{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/login",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/register",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/assets/*",
      "headers": {
        "cache-control": "public, max-age=31536000"
      }
    },
    {
      "route": "/vite.svg",
      "headers": {
        "cache-control": "public, max-age=31536000"
      }
    },
    {
      "route": "/manifest.json",
      "headers": {
        "cache-control": "public, max-age=31536000"
      }
    },
    {
      "route": "/sw.js",
      "headers": {
        "cache-control": "public, max-age=31536000"
      }
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "globalHeaders": {
    "Cache-Control": "no-cache"
  }
}